{
	"name": "vscode-foxdot",
	"icon": "images/vscodefoxdot_logo.png",
	"displayName": "VSCode-FoxDot",
	"description": "A FoxDot extension for Visual Studio Code",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:foxdot.start"
	],
	"main": "./foxdot.js",
	"contributes": {
		"commands": [
			{
				"command": "foxdot.start",
				"title": "FoxDot - Start"
			},
			{
				"command": "foxdot.stop",
				"title": "FoxDot - Stop"
			},
			{
				"command": "foxdot.openFolder",
				"title": "FoxDot - Open Folder"
			}
		],
		"keybindings": [
			{
				"command": "foxdot.evaluateSelection",
				"key": "ctrl+enter",
				"mac": "cmd+enter",
				"when": "editorTextFocus"
			}
		]
	},
	"publisher": "vvzen",
	"repository": {
		"url": "https://github.com/vvzen/vscode-foxdot"
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.2.0",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
